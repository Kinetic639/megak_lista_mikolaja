\";\r\nimport TextField from '@mui/material/TextField';\r\nimport Button from '@mui/material/Button';\r\nimport Grid from '@mui/material/Grid';\r\nimport { ChildGiftSelect } from '../ChildGiftSelect/ChildGiftSelect';\r\nimport {  useAppDispatch } from '../../app/hooks';\r\nimport FormControl from '@mui/material/FormControl';\r\nimport { toast } from 'react-toastify';\r\nimport { addChildAsync } from '../../redux/features/children-slice';\r\n\r\n\r\n\r\nexport const AddChild = () => {\r\n    const [nameError, setNameError] = useState<boolean>(false)  \r\n    const [selectedGiftId, setSelectedGiftId] = useState<string>('')\r\n    const isMounted = useRef(false);\r\n    const dispatch = useAppDispatch()\r\n\r\n    const initialFormState = {\r\nname: '',\r\ngiftId: selectedGiftId\r\n    }\r\n    \r\n    const [form, setForm] = useState<CreateChildReq>({\r\n        name: \"\",\r\n        giftId: selectedGiftId\r\n    })\r\n    const clearForm = () => {\r\n        setForm(initialFormState)\r\n        setSelectedGiftId('')\r\n    }\r\n\r\n    const updateForm = (key: string, value: any) => {\r\n        setForm(form => ({\r\n            ...form,\r\n            [key]: value,\r\n        }))\r\n    }\r\n\r\n    useEffect(() => {\r\n        if (isMounted.current) {\r\n            setForm({...form,\r\n                giftId: selectedGiftId,})   \r\n        } else {\r\n          isMounted.current = true;\r\n        }\r\n      }, [selectedGiftId]);\r\n\r\n\r\n    const sendForm = async (e: FormEvent) => {\r\n        \r\n        e.preventDefault()\r\n        setNameError(false)\r\n        \r\n        if (!form.name || form.name.length < 3 || form.name.length > 55) {\r\n            setNameError(true)\r\n            toast.error('Name must be between 3 and 55 letters long!')\r\n        } else {\r\n            dispatch(addChildAsync(form))\r\n            clearForm()\r\n        }\r\n    }\r\n\r\n\r\n    return <form noValidate autoComplete=\"off\" onSubmit={sendForm}>\r\n        <Grid container sx={{maxWidth: '800px', }} spacing={1}>\r\n            <Grid item xs={12}  md={5} >\r\n            <FormControl sx={{ m: 1 ,maxWidth: 460, minWidth: 250, width: '100%' }} size=\"small\">\r\n                <TextField\r\n                    value={form.name}\r\n                    label=\"Child's Name\"\r\n                    variant='outlined'\r\n                    color='primary'\r\n                    size='small'\r\n                    required\r\n                    error={nameError}\r\n                    InputProps={{ inputProps: { minLength: 3, maxLength: 25}}}\r\n                    onChange={e => updateForm('name', e.target.value)}/>\r\n                    </FormControl>\r\n            </Grid>\r\n            <Grid item  >\r\n                <ChildGiftSelect childId={''} selectedId={''} setSelectedGiftId={setSelectedGiftId}/>\r\n            </Grid>\r\n            <Grid item  sx={{display: \"flex\", alignItems: \"center\"}}>\r\n                <Button type=\"submit\" color=\"primary\" variant='contained'>Add Child</Button>\r\n            </Grid>\r\n\r\n        </Grid>\r\n\r\n    </form>\r\n}\r\n",[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\AddGift\\AddGift.tsx",[],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\Children\\ChildrenList.tsx",[],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\views\\Layout\\Appbar\\Appbar.tsx",[],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\Drawer\\Drawer.tsx",[],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\Gifts\\GiftsTable.tsx",["143"],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\Children\\ChildrenTable.tsx",[],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\comon\\Spinner\\Spinner.tsx",[],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\components\\ChildGiftSelect\\ChildGiftSelect.tsx",["144","145"],[],"D:\\projects\\megak_lista_mikołaja\\client\\src\\redux\\features\\gifts-slice.ts",[],[],"D:\\